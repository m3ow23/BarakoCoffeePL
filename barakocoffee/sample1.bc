public class Cat {
    private string name;

    public void setName(string name) {
        this.name = name;
    }

    public string getName() {
        return name;
    }

    // this method allows the cat to make a sound
    public void talk() {
        print("meow");
    }

    // this method allows the cat to walk
    public void walk(int meters) {
        print("The cat walked " + meters + " meters");
    }
}